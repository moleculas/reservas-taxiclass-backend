{
    "name": "taxiclass/reservas-api",
    "description": "API REST para sistema de reservas TaxiClass",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "firebase/php-jwt": "^6.0",
        "vlucas/phpdotenv": "^5.0",
        "respect/validation": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "api/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "disable-tls": true,
        "secure-http": false
    }
}
